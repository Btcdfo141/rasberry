{
  "domain": "rainmachine",
  "name": "RainMachine",
  "config_flow": true,
  "documentation": "https://www.home-assistant.io/integrations/rainmachine",
  "requirements": [
    "git+https://github.com/bachya/regenmaschine.git@sslv3#regenmaschine==1.0.0"
  ],
  "codeowners": ["@bachya"],
  "iot_class": "local_polling",
  "homekit": {
    "models": ["Touch HD", "SPK5"]
  },
  "zeroconf": [
    {
      "type": "_http._tcp.local.",
      "name": "rainmachine*"
    }
  ],
  "loggers": ["regenmaschine"],
  "version": "1.0.0"
}
