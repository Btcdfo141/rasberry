{
  "problemMatcher": [
    {
      "owner": "pylint-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\s([EF]\\d{4}):\\s(.+)$",
          "file": 1,
          "line": 3,
          "column": 4,
          "code": 5,
          "message": 6
        }
      ]
    },
    {
      "owner": "pylint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\s([CRW]\\d{4}):\\s(.+)$",
          "file": 1,
          "line": 3,
          "column": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
