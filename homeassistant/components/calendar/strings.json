        "description": {
          "name": "Description"
        },
        "end_time": {
          "name": "End time"
        },
        "location": {
          "name": "Location"
        },
        "messages": {
          "name": "Message"
        },
        "start_time": {
          "name": "Start time"
        }
      }
    }
